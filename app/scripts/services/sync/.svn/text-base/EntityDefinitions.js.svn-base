'use strict';
/*
 The default attribute for identifying an entity is its id. In case you need a different attribute, add an entry to definitions eg.
 {
 'Catalog': {
 key: 'catalogId'
 }
 }
 */
angular.module('conductivEcatalogApp').factory('EntityDefinitions', function () {
    var definitions = {
        'default': {
            key: 'id'
        }
    };
    return definitions;
});