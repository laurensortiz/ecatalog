<div class="container-fluid assortmentFeature" id="AssortmentLinesheetFeature">
    <div class="line-assortment-header">
        <div class="row-fluid" id="ls-topbar">
            <div class="tablist"><!-- removed class span4 -->
                <a class="navElement tab-moveLeft" ng-show="showLeftNavTabs" ng-click="navegationTabs.prev()">Move Left</a>
                <div class="taps-content">
                    <ul style="left:{{ positionStart }}px">
                        <li ng-repeat="assortment in listOfAssortments" ng-click="openAssortment(assortment.id)" class="{{ isActive(assortment.id) }}" >
                            <span >
                                <p long-tap-to-show-assortment-options="assortment">{{assortment.description}}</p>
                            </span>
                        </li>
                    </ul>
                </div>

                <a class="navElement tab-moveRight" ng-show="showRightNavTabs" ng-click="navegationTabs.next()">Move Right</a>
                <a class="navElement tab-addNew" ng-click="openAssortmentNewModal()">Add Tab</a>
            </div>

            <div class="order-header" ng-click="openOrderPopup()">
                <p>{{languagesJSON['assortmentEditLinesheet.order.header']}}</p>
            </div><!-- end .order-header -->

        </div>

    </div>

    <div class="line-assortment-summary-bar">
        <div class="row-fluid">

            <div class="lineSheetInfo">
                <div class="span1">
                    {{languagesJSON['assortmentEditLinesheet.style']}}
                </div>
                <div class="span1">
                    {{languagesJSON['assortmentEditLinesheet.style.colors']}}
                </div>
                <div class="span1">
                    {{languagesJSON['assortmentEditLinesheet.units']}}
                </div>
                <div class="span1 last">
                    {{languagesJSON['assortmentEditLinesheet.cost']}}
                </div>
            </div>

            <div class="row-fluid" id="ls-topbar-value">
                <div class="span1 counter" ng-bind="orderedProductStylesInCurrentAssortment.length">
                </div>
                <div class="span1 counter" ng-bind="(productStyles | filter:{availableOnCurrentAssortment: true}).length">
                </div>
                <div class="span1 counter" ng-bind="totalQuantityForAssortment">
                </div>
                <div class="span1 counter last" ng-bind="totalPriceForAssortment| currency">
                </div>
            </div>

        </div>
    </div><!-- end .line-assortment-summary-bar -->

    <div class="assortment-list-container bg-white" draggable="false">
        <div class="initial-state" ng-hide="orderedProductStylesInCurrentAssortment.length > 0">
            <script type="text/ng-template" id="initialElementsLink">
                <a class="initialElementsLink btn btn-block {{initialElement.icon}}" href="{{initialElement.link}}" title="{{initialElement.text}}" ng-click="initialElement.action()">
                <span class="button-text">
                    <span> {{initialElement.text}} </span>
                </span>
                </a>
            </script>
            <span ng-repeat="initialElement in initialElements" ng-include="initialElement.type"></span>
        <h3>{{languagesJSON['assortmentEditLinesheet.add.products']}}</h3>
        </div>
        <div class="shoe-container" ng-repeat="productCodeAndStylePair in orderedProductStylesInCurrentAssortment">
            <div class="row-fluid procItems bg-white">
                <div class="span12" ng-click="togglePsVisibility(productCodeAndStylePair[0], $event)" hm-swipeleft="showDeleteButton[productCodeAndStylePair[0]] = false"
                     hm-swiperight="showDeleteButton[productCodeAndStylePair[0]] = true">
                    <div class="span2">
                        <img class="product-image" ng-src="{{productCodeAndStylePair[1][0].links.image}}"/>
                        <span class="product-length-bubble">{{productCodeAndStylePair[1].length}}</span>
                    </div>
                    <div class="span5">
                        <p class="virtual-product-style-name-container product-style-info">
                            {{productCodeAndStylePair[1][0].code}}
                        </p>
                    </div>
                    <div class="span1 offset1">
                        <div class="summary currQuantity" ng-bind="quantityPriceCache.virtualProducts[productCodeAndStylePair[0]].quantity"></div>
                    </div>
                    <div class="span1 last">
                        <div class="summary currPrice"
                             ng-bind="quantityPriceCache.virtualProducts[productCodeAndStylePair[0]].price | currency"></div>
                    </div>
                    <div class="span2">
                        <button ng-show="showDeleteButton[productCodeAndStylePair[0]]"
                                class="btn delete-button"
                                ng-click="removeVirtualProduct(productCodeAndStylePair[0],$event)">
                {{languagesJSON['assortmentEditLinesheet.delete']}}
                        </button>
                    </div>
                </div>
            </div>
            <div id="child-cont inicial" class="procStyle" ng-class="{depth: productStylesVisible[productCodeAndStylePair[0]], initial:!productStylesVisible[productCodeAndStylePair[0]]}">
                <div ng-repeat="productStyle in productCodeAndStylePair[1]">
                    <div class="procItems" ui-if="productStylesVisible[productCodeAndStylePair[0]]">
                        <div ng-hide="productStyle">
                            <img src="images/bx_loader.gif"/>
                        </div>
                        <div ng-include src="'partials/linesheetProductStyle.html'" ui-if="productStyle">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <numpad data="numpad" visibility="numpadVisible"></numpad>
    <ng-include src="'views/_browse-catalogs-modal.html'"></ng-include>

    <div modal="productdeletemodal" close="closeDeleteDialog()" options="opt">
    </div>
</div>
<div ng-include src="'partials/orderPopup.html'" ng-controller="OrderPopupCtrl">
</div>

<div ng-include src="'partials/assortmentNewPopup.html'" ng-controller="AssortmentActionsCtrl"></div>
<div ng-include src="'partials/assortmentRenameDialog.html'" ng-controller="AssortmentActionsCtrl"></div>
<div ng-include src="'partials/assortmentDeleteDialog.html'" ng-controller="AssortmentActionsCtrl"></div>
<div ng-include src="'partials/assortmentDuplicateDialog.html'" ng-controller="AssortmentActionsCtrl"></div>
<div ng-include src="'partials/assortmentActionsPopup.html'" ng-controller="AssortmentActionsCtrl"></div>

<ng-include src="'views/productSearch.html'"></ng-include>